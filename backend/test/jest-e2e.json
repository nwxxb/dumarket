{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "setupFilesAfterEnv": ["<rootDir>/setup.ts"],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/../src/$1",
    "^generated/(.*)$": "<rootDir>/../generated/$1"
  },
  "coverageDirectory": "<rootDir>/coverage/e2e"
}
